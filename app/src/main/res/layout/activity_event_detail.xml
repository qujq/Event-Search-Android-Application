<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/detail_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EventDetail">

    <TextView
        android:id="@+id/detail_seat_map_content"
        android:layout_width="206dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="28dp"
        android:text="TextView"
        android:textColor="#878787"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/detail_ticket_status3"
        app:layout_constraintTop_toBottomOf="@+id/detail_buy_ticket_at_content" />

    <TextView
        android:id="@+id/detail_buy_ticket_at_content"
        android:layout_width="206dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="28dp"
        android:text="TextView"
        android:textColor="#878787"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/detail_ticket_status2"
        app:layout_constraintTop_toBottomOf="@+id/detail_ticket_status_content" />

    <TextView
        android:id="@+id/detail_ticket_status3"
        android:layout_width="169dp"
        android:layout_height="39dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        android:text="Seat Map"
        android:textColor="#878787"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/detail_seat_map_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_buy_ticket_at_content" />

    <TextView
        android:id="@+id/detail_ticket_status2"
        android:layout_width="169dp"
        android:layout_height="39dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        android:text="Buy Ticket At"
        android:textColor="#878787"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/detail_buy_ticket_at_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_ticket_status_content" />

    <TextView
        android:id="@+id/detail_ticket_status_content"
        android:layout_width="207dp"
        android:layout_height="31dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="28dp"
        android:text="TextView"
        android:textColor="#878787"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_price_range_content" />

    <TextView
        android:id="@+id/detail_ticket_status"
        android:layout_width="169dp"
        android:layout_height="39dp"
        android:layout_marginTop="28dp"
        android:text="Ticket Status"
        android:textColor="#878787"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/detail_ticket_status_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_price_range_content" />

    <TextView
        android:id="@+id/detail_price_range_content"
        android:layout_width="206dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="28dp"
        android:text="TextView"
        android:textColor="#878787"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/detail_price_range"
        app:layout_constraintTop_toBottomOf="@+id/detail_category_content" />

    <TextView
        android:id="@+id/detail_price_range"
        android:layout_width="169dp"
        android:layout_height="39dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        android:text="Price Range"
        android:textColor="#878787"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/detail_price_range_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_category_content" />

    <TextView
        android:id="@+id/detail_category_content"
        android:layout_width="206dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="28dp"
        android:text="TextView"
        android:textColor="#878787"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/detail_category"
        app:layout_constraintTop_toBottomOf="@+id/detail_date_content" />

    <TextView
        android:id="@+id/detail_category"
        android:layout_width="169dp"
        android:layout_height="39dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        android:text="Category"
        android:textColor="#878787"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/detail_category_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_date_content" />

    <TextView
        android:id="@+id/detail_date_content"
        android:layout_width="206dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="28dp"
        android:text="TextView"
        android:textColor="#878787"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/detail_date"
        app:layout_constraintTop_toBottomOf="@+id/detail_venue_content" />

    <TextView
        android:id="@+id/detail_date"
        android:layout_width="169dp"
        android:layout_height="39dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        android:text="Date"
        android:textColor="#878787"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/detail_date_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_venue_content" />

    <TextView
        android:id="@+id/detail_venue"
        android:layout_width="169dp"
        android:layout_height="39dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        android:text="Venue"
        android:textColor="#878787"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_artists_teams_content" />

    <TextView
        android:id="@+id/detail_artists_teams2"
        android:layout_width="169dp"
        android:layout_height="39dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="80dp"
        android:text="Artist(s)/Teams"
        android:textColor="#878787"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/detail_artists_teams_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/detail_artists_teams_content"
        android:layout_width="206dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="28dp"
        android:text="TextView"
        android:textColor="#878787"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/detail_artists_teams2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/detail_venue_content"
        android:layout_width="206dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="TextView"
        android:textColor="#878787"
        android:textSize="22sp"
        app:layout_constraintStart_toEndOf="@+id/detail_venue"
        app:layout_constraintTop_toBottomOf="@+id/detail_artists_teams_content" />


</androidx.constraintlayout.widget.ConstraintLayout>